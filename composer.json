{
    "name": "eduzz/hermes",
    "description": "Essa Lib e responsavel por encapsular a conexao com gerenciadores de filas. Focando no uso com RabbitMQ.",
    "keywords": ["library", "rabbitmq", "queue", "Eduzz"],
    "license": "MIT",
    "type": "library",
     "require": {
        "php": ">=7.0",
        "ext-bcmath": "*",
        "ext-mbstring": "*",
        "php-amqplib/php-amqplib": "^2.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Eduzz\\Hermes\\": "src/",
            "Eduzz\\Hermes\\Examples\\": "examples/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eduzz\\Hermes\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/eduzz/hermes.git"
        }
    ]
}
