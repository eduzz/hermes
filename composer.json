{
    "name": "eduzz/hermes",
    "description": "Essa Lib e responsavel por encapsular a conexao com gerenciadores de filas. Focando no uso com RabbitMQ.",
    "keywords": ["library", "rabbitmq", "queue", "Eduzz"],
    "authors": [
        {
            "name": "Bruno Augusto Guido",
            "email": "bruno.guido@eduzz.com"
        },
        {
            "name": "Luiz Fernando de Oliveira da Silva",
            "email": "luiz.silva@eduzz.com"
        },
        {
            "name": "Angelo Rodrigo Ribeiro da Silva",
            "email": "angelo.silva@eduzz.com"
        }
    ],
    "license": "MIT",
    "type": "library",
     "require": {
        "php": ">=5.3.0",
        "ext-bcmath": "*",
        "ext-mbstring": "*",
        "php-amqplib/php-amqplib": "2.7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Eduzz\\Hermes\\": "src/",
            "Eduzz\\Hermes\\Examples\\": "examples/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eduzz\\Hermes\\Tests\\": "tests/"
        }
    }
}
